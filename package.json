{
  "name": "invoice-app-remix-33ca",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -i ./app/styles/tailwind.input.css -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook",
    "chromatic": "npx chromatic"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "@radix-ui/react-dialog": "^0.1.8-rc.48",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.6.0",
    "@remix-run/serve": "^1.6.0",
    "@remix-run/server-runtime": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-merge-refs": "^2.0.0",
    "remix-utils": "^3.3.0",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "^1.6.0",
    "@remix-run/eslint-config": "^1.6.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.8",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.8",
    "@storybook/react": "^6.5.9",
    "@storybook/test-runner": "^0.1.1",
    "@storybook/testing-library": "^0.0.12",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "axe-playwright": "^1.1.11",
    "babel-loader": "^8.2.5",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "chromatic": "^6.6.3",
    "cross-env": "^7.0.3",
    "cypress": "^10.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.12",
    "happy-dom": "^5.3.1",
    "jest": "^27.5.1",
    "msw": "^0.42.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^3.15.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "type-fest": "^2.13.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.15.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
